<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
  <!--
  BackupAlgoriths
      ,GFS    (Grandfather, Father, son: Keeps Daily's up to DailyMaxDaysOld, Keeps Weekly's up to WeeklyMaxDaysOld, Keeps Monthly's up to MonthlyMaxDaysOld )
      ,KeepAll    (No Deleting)
      ,KeepDaily  (Keep Daily's up to DailyMaxDaysOld)
      ,KeepWeekly (Only Keep Weekly Backups up to WeeklyMaxDaysOld)
      ,KeepMonthly (Only Keep Monthly Backups up to MonthlyMaxDaysOld)
      
  FileSyncReplicaOption
    OneWay (One Way Sync)
    ,TwoWay (Two Way Sync)
    
   Time Format (Military Time): "00:00"
   
   Compress,Encrypt,SyncFolders: "true" or "false"
   
   Service Interval is in 1000 of a second e.g. "120000" = 2 minutes
  -->

  <appSettings>
    <add key="ServiceInterval" value="120000"/>
    <add key="RetentionExecutionTime" value="16:15"/>
    <add key="CompressExecutionTime" value="16:20"/>
    <add key="SyncFolderExecutionTime" value="08:09"/>
    <add key="MaxDriveSpaceUsedPercent" value="0.95"/>
  </appSettings>
    
</configuration>