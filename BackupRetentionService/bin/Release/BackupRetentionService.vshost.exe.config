<?xml version="1.0"?>
<configuration>
    <configSections>
    </configSections>
  <!--
   Service Interval is in 1000 of a second e.g. "120000" = 2 minutes
  -->

  <connectionStrings>
    <add name="BackupRetention.Properties.Settings.BackupRetentionConnectionString"
      connectionString="Data Source=|DataDirectory|\BackupRetention.sdf"
      providerName="Microsoft.SqlServerCe.Client.3.5" />
  </connectionStrings>
  <appSettings>
    <add key="ServiceInterval" value="120000"/>
    <add key="MaxDriveSpaceUsedPercent" value="0.95"/>
  </appSettings>
    
</configuration>
